    },
    "voidBreath": {
      "mouthOpen": { "part": "voidMouth", "type": "scale", "amount": 1.5, "duration": 0.3 },
      "chargeUp": { "part": "mouthGlow", "type": "scale", "amount": 3.0, "duration": 1.5 },
      "chargeGlow": { "part": "mouthGlow", "type": "emissive", "intensity": 8.0, "duration": 1.5 },
