      "tendrilSway": { "parts": ["tendril1", "tendril2", "tendril3"], "type": "rotate", "axis": "z", "amount": 15, "speed": 0.5 },
      "eyeWander": { "parts": ["eyeBall", "eyePupil"], "type": "look-random", "interval": 3.0 },
      "breathe": { "part": "spinalColumn", "type": "scale", "axis": "x", "min": 0.95, "max": 1.05, "speed": 0.8 }
    },
    "targeting": {
      "eyeLock": { "parts": ["eyeBall", "eyePupil"], "type": "lookAt", "speed": 5.0 },
      "pupilDilate": { "part": "eyePupil", "type": "scale", "amount": 1.5, "duration": 0.2 },
      "tendrilsPoint": { "parts": ["tendril1", "tendril2", "tendril3"], "type": "lookAt", "speed": 2.0 },
      "heartRace": { "part": "heartCore", "type": "scale", "speed": 3.0 }
    },
    "firing": {
      "acidLaunch": { "parts": ["acidSac1", "acidSac2"], "type": "scale", "amount": 0.5, "duration": 0.15, "return": 0.3 },
      "tendrilStrike": { "parts": ["tendril1", "tendril2", "tendril3"], "type": "extend", "amount": 1.5, "duration": 0.2 },
      "heartFlare": { "part": "heartCore", "type": "emissive", "intensity": 5.0, "duration": 0.1 },
      "eyeFlash": { "part": "eyeBall", "type": "emissive", "color": "#ff0000", "intensity": 4.0, "duration": 0.1 }
    },
    "damaged": {
      "ichor": { "type": "particles", "count": 15, "color": "#44ff00", "spread": 2.0 },
      "spasm": { "parts": ["tendril1", "tendril2", "tendril3"], "type": "shake", "intensity": 0.3 },
      "heartSkip": { "part": "heartCore", "type": "flicker", "duration": 0.5 }
    },
    "destroyed": {
      "deathRattle": { "type": "sound", "id": "organic-death" },
      "ichorExplosion": { "type": "particles", "count": 80, "colors": ["#44ff00", "#88ff44", "#ff4444"] },
      "tendrilCollapse": { "parts": ["tendril1", "tendril2", "tendril3"], "type": "physics", "gravity": true },
      "coreRupture": { "part": "heartCore", "type": "explode", "force": 10 }
    }
  },
