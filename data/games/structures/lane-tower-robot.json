    "idle": {
      "sensorRotate": { "part": "sensorArray", "axis": "y", "speed": 2.0 },
      "corePulse": { "part": "energyCore", "type": "scale", "min": 0.9, "max": 1.1, "speed": 1.5 },
      "shieldGlow": { "parts": ["shieldEmitter1", "shieldEmitter2", "shieldEmitter3", "shieldEmitter4"], "type": "emissive", "min": 0.8, "max": 1.5, "speed": 2.0 },
      "statusBlink": { "parts": ["statusLight1", "statusLight2"], "type": "blink", "interval": 1.0 }
    },
    "targeting": {
      "turretTrack": { "part": "turretHead", "type": "lookAt", "speed": 3.0 },
      "cannonAim": { "parts": ["cannonLeft", "cannonRight"], "type": "lookAt", "speed": 3.0 },
      "sensorLock": { "part": "sensorArray", "type": "emissive", "color": "#ff0000", "intensity": 3.0 }
    },
    "firing": {
      "cannonRecoil": { "parts": ["cannonLeft", "cannonRight"], "type": "translate", "axis": "z", "amount": -0.3, "duration": 0.1, "return": 0.2 },
      "muzzleFlash": { "parts": ["cannonMuzzleLeft", "cannonMuzzleRight"], "type": "emissive", "intensity": 5.0, "duration": 0.05 },
      "coreFlare": { "part": "energyCore", "type": "scale", "amount": 1.3, "duration": 0.1 }
    },
    "damaged": {
      "sparks": { "type": "particles", "count": 20, "spread": 2.0 },
      "smoke": { "type": "particles", "count": 10, "color": "#333333" },
      "flicker": { "parts": ["statusLight1", "statusLight2"], "type": "flicker", "speed": 10 }
    },
    "destroyed": {
      "explosion": { "type": "particles", "count": 100, "spread": 5.0, "colors": ["#ff4400", "#ffaa00", "#ffffff"] },
      "collapse": { "type": "physics", "gravity": true, "duration": 2.0 },
      "debris": { "type": "spawn", "count": 15, "lifetime": 3.0 }
    }
  },
  "stats": {
    "health": 2500,
