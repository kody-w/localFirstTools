  "animations": {
    "idle": {
      "coreFloat": { "part": "quantumCore", "type": "translate", "axis": "y", "min": -0.3, "max": 0.3, "speed": 0.5 },
      "coreRotate": { "part": "quantumCore", "type": "rotate", "axis": "y", "speed": 0.2 },
      "shellRotate": { "part": "coreShell", "type": "rotate", "axes": ["x", "y"], "speed": 0.1 },
      "ringRotate1": { "part": "dataRing1", "type": "rotate", "axis": "y", "speed": 1.0 },
      "ringRotate2": { "part": "dataRing2", "type": "rotate", "axis": "y", "speed": -0.8 },
      "energyFlow": { "part": "energyMoat", "type": "uv-scroll", "speed": 1.0 },
      "pylonPulse": { "parts": ["pylonOrb1", "pylonOrb2", "pylonOrb3", "pylonOrb4"], "type": "emissive", "min": 1.5, "max": 2.5, "speed": 1.5, "stagger": 0.25 },
      "shieldShimmer": { "part": "shieldDome", "type": "opacity", "min": 0.08, "max": 0.15, "speed": 0.3 },
      "beaconPulse": { "part": "beaconLight", "type": "emissive", "min": 2.0, "max": 4.0, "speed": 2.0 },
      "displayScroll": { "parts": ["holoDisplay1", "holoDisplay2"], "type": "uv-scroll", "speed": 0.5 }
    },
    "alert": {
      "coreAccelerate": { "part": "quantumCore", "type": "rotate", "axis": "y", "speed": 2.0 },
      "ringSpeedup": { "parts": ["dataRing1", "dataRing2"], "type": "rotate", "speed": 3.0 },
      "shieldIntensify": { "part": "shieldDome", "type": "emissive", "intensity": 1.5 },
      "beaconRapid": { "part": "beaconLight", "type": "blink", "interval": 0.3 },
      "pylonCharge": { "parts": ["pylonOrb1", "pylonOrb2", "pylonOrb3", "pylonOrb4"], "type": "emissive", "intensity": 4.0 }
    },
    "damaged": {
      "coreFlicker": { "part": "quantumCore", "type": "flicker", "speed": 8 },
      "sparks": { "type": "particles", "count": 40, "colors": ["#00ffff", "#ffffff", "#ffaa00"] },
      "shieldCrack": { "part": "shieldDome", "type": "flicker", "speed": 15 },
      "displayStatic": { "parts": ["holoDisplay1", "holoDisplay2"], "type": "noise", "intensity": 0.5 }
    },
    "destroyed": {
      "coreOverload": { "part": "quantumCore", "type": "explode", "force": 30, "color": "#00ffff" },
      "pylonCollapse": { "parts": ["supportPylon1", "supportPylon2", "supportPylon3", "supportPylon4"], "type": "physics", "gravity": true },
      "massiveExplosion": { "type": "particles", "count": 300, "colors": ["#00ffff", "#00ff88", "#ffffff", "#ffaa00"], "spread": 15 },
      "shockwave": { "type": "shockwave", "radius": 20, "color": "#00ffff" },
      "structureCollapse": { "type": "physics", "gravity": true, "duration": 5.0 }
    }
  },
  "stats": {
