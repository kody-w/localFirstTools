  "animations": {
    "idle": {
      "hover": { "part": "chassisBase", "type": "translate", "axis": "y", "min": -0.05, "max": 0.05, "speed": 1.5 },
      "scanPulse": { "part": "visor", "type": "emissive", "min": 1.5, "max": 3.0, "speed": 2.0 },
      "healReady": { "parts": ["healEmitterLeft", "healEmitterRight"], "type": "emissive", "min": 2.0, "max": 3.0, "speed": 1.5 },
      "powerPulse": { "part": "powerCell", "type": "emissive", "min": 1.5, "max": 2.5, "speed": 1.0 },
      "antennaGlow": { "part": "antennaLight", "type": "blink", "interval": 2.0 },
      "naniteSwirl": { "parts": ["naniteCanisterLeft", "naniteCanisterRight"], "type": "particles", "count": 2, "color": "#00ff88", "internal": true }
    },
    "move": {
      "tiltForward": { "part": "chassisCore", "type": "rotate", "axis": "x", "amount": 10 },
      "armBalance": { "parts": ["armLeft", "armRight"], "type": "rotate", "axis": "x", "min": -15, "max": 15, "speed": 1.0, "phase": 0.5 },
      "hoverTrail": { "type": "particles", "count": 3, "color": "#00ffff", "trail": true }
    },
    "heal": {
      "armExtend": { "parts": ["armLeft", "armRight"], "type": "rotate", "toTarget": true },
      "emitterCharge": { "parts": ["healEmitterLeft", "healEmitterRight"], "type": "emissive", "intensity": 5.0, "duration": 0.3 },
      "healBeam": { "type": "beam", "origin": ["healEmitterLeft", "healEmitterRight"], "color": "#00ff88", "width": 0.15 },
      "naniteSpray": { "type": "particles", "count": 30, "color": "#00ff88", "toTarget": true },
      "crossGlow": { "parts": ["medicalCross", "medicalCrossVertical"], "type": "emissive", "intensity": 4.0 }
    },
    "shield": {
      "projectorActivate": { "part": "shieldProjector", "type": "emissive", "intensity": 5.0 },
      "shieldDeploy": { "type": "sphere", "color": "#0088ff", "radius": 3, "opacity": 0.3 },
      "shieldPulse": { "type": "ring", "color": "#0088ff", "expand": true }
    },
    "damaged": {
      "sparks": { "type": "particles", "count": 20, "colors": ["#ffffff", "#00ff88"] },
      "canisterLeak": { "parts": ["naniteCanisterLeft", "naniteCanisterRight"], "type": "particles", "count": 5, "color": "#00ff88" },
      "visorFlicker": { "part": "visor", "type": "flicker", "speed": 10 }
    },
    "destroyed": {
      "naniteBurst": { "type": "particles", "count": 100, "color": "#00ff88", "spread": 5 },
      "powerOverload": { "part": "powerCell", "type": "emissive", "intensity": 8.0, "duration": 0.3 },
      "explosion": { "type": "explode", "force": 12, "color": "#00ffff" },
      "debris": { "type": "physics", "gravity": true, "parts": ["headDome", "armLeft", "armRight", "backpackUnit"] },
      "healingPulse": { "type": "ring", "color": "#00ff88", "radius": 8, "heals": true }
    }
  },
