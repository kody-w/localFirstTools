    },
    "move": {
      "trackRoll": { "parts": ["trackLeft", "trackRight"], "type": "uv-scroll", "speed": 2.0 },
      "wheelSpin": { "parts": ["roadWheelLeft1", "roadWheelLeft2", "roadWheelLeft3", "roadWheelLeft4"], "type": "rotate", "axis": "x", "speed": 3.0 },
      "bodyRock": { "part": "hullBase", "type": "rotate", "axis": "x", "min": -2, "max": 2, "speed": 1.0 },
      "dustTrail": { "type": "particles", "count": 15, "color": "#886644", "position": "rear" }
    },
    "attack": {
      "turretAim": { "part": "turretBody", "type": "rotate", "axis": "y", "toTarget": true, "speed": 2.0 },
      "coilOvercharge": { "parts": ["railgunCoil1", "railgunCoil2", "railgunCoil3"], "type": "emissive", "intensity": 6.0, "sequence": true, "duration": 0.3 },
      "fireRecoil": { "parts": ["turretBody", "railgunBarrel"], "type": "translate", "axis": "z", "amount": -0.3, "duration": 0.1, "return": 0.3 },
      "muzzleBlast": { "part": "railgunMuzzleGlow", "type": "scale", "amount": 3.0, "emissive": 10.0, "duration": 0.1 },
      "railgunShot": { "type": "beam", "origin": "railgunMuzzle", "color": "#00ffff", "width": 0.2, "length": 50 },
      "shockwave": { "type": "ring", "position": "railgunMuzzle", "color": "#00ffff", "radius": 3, "duration": 0.3 },
      "shellEject": { "type": "particles", "count": 1, "color": "#ffaa00", "position": "turretBody", "physics": true }
    },
    "damaged": {
      "armorSpark": { "type": "particles", "count": 30, "colors": ["#ffaa00", "#ffffff"] },
      "reactiveBlast": { "type": "explosion", "force": 3, "color": "#ffaa00", "position": "hit" },
      "trackDamage": { "parts": ["trackLeft", "trackRight"], "type": "flicker", "speed": 5, "duration": 0.3 },
      "sensorGlitch": { "part": "sensorLens", "type": "flicker", "speed": 12 }
    },
    "destroyed": {
      "turretBlowoff": { "parts": ["turretBody", "railgunBarrel", "railgunCoil1", "railgunCoil2", "railgunCoil3"], "type": "physics", "force": 20, "upward": true },
      "hullExplosion": { "type": "explode", "force": 25, "color": "#ffaa00" },
      "secondaryExplosions": { "type": "explosion", "count": 3, "delay": 0.2, "spread": 2, "color": "#ff4400" },
      "fireball": { "type": "particles", "count": 150, "colors": ["#ff4400", "#ffaa00", "#ffffff", "#00ffff"], "spread": 8 },
      "debris": { "type": "physics", "gravity": true, "parts": ["reactiveArmorLeft1", "reactiveArmorRight1", "trackGuardLeft", "trackGuardRight"] },
      "smokePlume": { "type": "particles", "count": 50, "color": "#222222", "rise": true, "duration": 5.0 }
    }
  },
