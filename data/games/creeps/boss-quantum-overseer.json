      "hover": { "part": "quantumCore", "type": "translate", "axis": "y", "min": -0.2, "max": 0.2, "speed": 0.5 }
    },
    "move": {
      "bankLeft": { "parts": ["dataRing1", "dataRing2", "dataRing3"], "type": "rotate", "axis": "z", "amount": 15 },
      "thrusterBoost": { "parts": ["thrusterGlow1", "thrusterGlow2", "thrusterGlow3"], "type": "scale", "amount": 1.5 },
      "streamTrail": { "type": "particles", "count": 10, "color": "#00ffff", "trail": true }
    },
    "attack": {
