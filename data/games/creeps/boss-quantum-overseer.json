      "quantumLaser": { "type": "beam", "origin": "sensorEye", "color": "#ff0000", "width": 0.3, "duration": 1.5, "damage": 150 },
      "droneSwarm": { "type": "spawn", "model": "robot-scout-drone", "count": 4, "positions": ["droneMount1", "droneMount2"] }
    },
    "shieldPhase": {
      "shieldActivate": { "part": "shieldEmitter", "type": "opacity", "target": 0.4, "duration": 0.5 },
      "shieldPulseIntense": { "part": "shieldEmitter", "type": "emissive", "intensity": 3.0 },
      "reflectMode": { "type": "damage-reflect", "percentage": 50, "duration": 5.0 }
    },
    "damaged": {
      "glitch": { "parts": ["quantumCore", "outerShell"], "type": "flicker", "speed": 20, "duration": 0.3 },
      "sparks": { "type": "particles", "count": 30, "colors": ["#00ffff", "#ffaa00", "#ffffff"] },
      "ringDisrupt": { "parts": ["dataRing1", "dataRing2", "dataRing3"], "type": "jitter", "intensity": 0.2 },
      "shieldFlash": { "part": "shieldEmitter", "type": "opacity", "target": 0.3, "duration": 0.1 }
    },
    "destroyed": {
      "coreOverload": { "part": "quantumCore", "type": "emissive", "target": 15.0, "duration": 1.0 },
      "ringsShatter": { "parts": ["dataRing1", "dataRing2", "dataRing3"], "type": "explode", "force": 10 },
      "shellBreak": { "part": "outerShell", "type": "physics", "fragments": 12 },
      "quantumExplosion": { "type": "particles", "count": 200, "colors": ["#00ffff", "#00ff88", "#ff8800", "#ffffff"], "spread": 12 },
      "empWave": { "type": "shockwave", "color": "#00ffff", "radius": 25, "stun": true }
    }
