{
  "dashboard": {
    "title": "VM Abstraction Levels",
    "panels": [
      {
        "title": "Abstraction Levels Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{level}} - {{type}}"
          }
        ]
      },
      {
        "title": "Resource Usage by Level",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes",
            "legendFormat": "Memory - {{name}}"
          }
        ]
      }
    ]
  }
}
